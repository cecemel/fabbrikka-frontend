<div class="row">
{{fabb-carousel}}
</div>
<div class="row">
    {{#unless media.isM }}
      <h1 class="header center-align">{{t "component.title.upper"}} - {{t "component.title.lower"}}</h1>
     {{else}}
      <h6 class="header center-align">{{t "component.title.upper"}}</h6>
      <h1 class="center-align">{{t "component.title.lower"}}</h1>
     {{/unless}}
      {{#items-grid data=model itemComponentName="product-card"  onClickLinkTo="products.details"}} {{/items-grid}}
</div>
{{outlet}}
